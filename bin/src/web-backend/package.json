{
  "name": "talentbook",
  "version": "1.0.0",
  "description": "Coding Dojo Talentbook",
  "main": "app.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha $*",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/app.js",
    "dev": "NODE_ENV=development nodemon --exec babel-node server/app.js",
    "transpile": "babel ./server --out-dir dist-server && cp ./server/config/*.yml ./dist-server/config/",
    "clean": "rimraf dist-server",
    "watch": "nodemon",
    "doc": "./node_modules/.bin/jsdoc -d docs --configure jsdoc_conf.json -r server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "watch": [
      "server/config",
      "server/controllers",
      "server/helpers",
      "server/middleware",
      "server/models",
      "server/routes",
      "server/app.js"
    ],
    "ignore": [
      "server/.git",
      "node_modules",
      "server/assets",
      "server/public/assets/cache",
      "server/config/migrations",
      "server/views",
      "server/tests",
      "server/public",
      "server/test",
      "server/scripts",
      "server/sessions"
    ]
  },
  "devDependencies": {
    "mocha": "^7.2.0",
    "nodemon": "^2.0.5",
    "selenium-webdriver": "^4.0.0-alpha.7"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@octokit/core": "^3.2.4",
    "@sendgrid/mail": "^7.3.0",
    "@sentry/node": "^5.29.0",
    "@sentry/tracing": "^5.29.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "async-request": "^1.2.0",
    "aws-sdk": "^2.772.0",
    "base64-url": "^2.3.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.17.1",
    "express-device": "^0.4.2",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "fs-extra": "^9.0.1",
    "jquery": "^3.5.1",
    "js-yaml": "^3.14.0",
    "jsdoc": "^3.6.6",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "memorystore": "^1.6.2",
    "mime-types": "^2.1.35",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "multer-s3-transform": "^2.10.3",
    "mysql": "^2.17.1",
    "node-async-loop": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.27.2",
    "superagent": "^6.1.0",
    "zombie": "^6.1.4"
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}
